{
  "name": "@zoranwong/acc-engine.js",
  "version": "0.0.1",
  "description": "A javascript framework to accelerate your applicatin developement",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles ./tests/",
    "watch": "webpack --mode development --watch",
    "start": "webpack --mode development",
    "deploy": "webpack --mode production"
  },
  "keywords": [
    "Javascript",
    "js",
    "framework",
    "container",
    "accelerate",
    "node.js"
  ],
  "author": "Zoran Wong",
  "license": "MIT",
  "dependencies": {
    "@zoranwong/pure-container": "0.0.24",
    "md5": "^2.2.1",
    "workerpool": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.6",
    "babel-eslint": "v11.0.0-beta.2",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.3.0",
    "eslint": "^7.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-private-props": "^0.3.0",
    "jest": "^25.4.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "experimentalObjectRestSpread": true
    },
    "plugins": [
      "private-props"
    ],
    "rules": {
      "private-props/no-unused-or-undeclared": [
        2,
        {
          "privateMatchers": [
            "^_",
            "^handle[A-Z]"
          ]
        }
      ]
    }
  }
}
